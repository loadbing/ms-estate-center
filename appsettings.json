{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongoDB": {
    "ConnectionString": "${MONGO_DB_CONNECTION_STRING}",
    "DatabaseName": "${MONGO_DB_DATABASE_NAME}"
  },
  "Jwt": {
    "Key": "${JWT_KEY}"
  },
  "AESSettings": {
    "Key": "${AES_SETTINGS_KEY}",
    "IV": "${AES_SETTINGS_IV}"
  },
  "AllowedHosts": "*"
}
